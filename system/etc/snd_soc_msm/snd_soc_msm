SectionModifier
	Name "Play Tunnel"

	EnableSequence
		'SLIMBUS_0_RX Audio Mixer MultiMedia4':1:1
	EndSequence

	DisableSequence
		'SLIMBUS_0_RX Audio Mixer MultiMedia4':1:0
	EndSequence

	# ALSA PCMs
	PlaybackPCM 9

EndSection

